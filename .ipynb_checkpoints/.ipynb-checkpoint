    "def counter(number):\n",
    "    if number <=1:\n",
    "        return 1\n",
    "    else:\n",
    "        return number * counter(number-1)\n",
    "\n",
    "number =range(1,6)\n",
    "sum([counter(one_number)\n",
    "for one_number in number])\n",
    "\n"
   ]
